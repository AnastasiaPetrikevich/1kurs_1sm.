#include <iostream>
using namespace std;

int FibonachiNumber(int n);

int main()
{
	long long n = 0;
	cout << "Enter you number: "<< endl;
	cin >> n;
	cout << "Fibonachi number = " << FibonachiNumber(n) << endl;
	system("pause");
	return 0;
}

int FibonachiNumber(int n)
{
	if (n <2)
		return 1;
	else return FibonachiNumber(n - 2) + FibonachiNumber(n - 1);
}
